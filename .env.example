# Google API Key (Gemini 모델 및 이미지 분석용)
GOOGLE_API_KEY=your_google_api_key_here

# Upstage API Key (QueryBuilder의 Solar Pro 3 쿼리 정제용)
UPSTAGE_API_KEY=your_upstage_api_key_here

# 프로젝트 루트 데이터 디렉토리 (ChromaDB, Musinsa JSON 등 포함)
PATH_DATA_ROOT=./data

# ChromaDB 경로
PATH_CHROMA=./data/chroma_db

# 세션 및 유저 프로필 저장 경로
PATH_STORAGE=./data/storage


# 최대 재생성(REGENERATE) 시도 횟수
MAX_REGENERATE_COUNT=2

# 옷장 매칭 임계값 (0.0 ~ 1.0, 높을수록 엄격)
WARDROBE_MATCH_THRESHOLD=0.4

# 무신사 상품 추천 임계값
BUYING_MATCH_THRESHOLD=0.3
